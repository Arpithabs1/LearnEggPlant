(******
HandlerName: clearBrowserHistory
Input: None
Output: Clears browser history(firefox)
Description: Clears browser history(firefox)
*******)

on clearBrowserHistory
	TypeText windowsKey & "r"
	TypeText "firefox.exe", return
	if(ImageFound(3, "BEH_SA_CommonHandlers/Button_Maximize"))
		Click "BEH_SA_CommonHandlers/Button_Maximize"
	end if	
	wait 4
	click("BEH_SA_CommonHandlers/Button_ViewBrowserHistory")
	click(Text:"History",dpi:100)
	wait 1
	click(Text:"Clear Recent History...",dpi:100)
	wait 1
	put ImageLocation(Text:"Time range to clear:")+(150,0) into DropDown
	click DropDown
	click(Text:"Everything",dpi:100)
	wait 1
	click (Text:"Clear Now",dpi:100)
	click ("BEH_SA_CommonHandlers/Button_BrowserClose")
end clearBrowserHistory
