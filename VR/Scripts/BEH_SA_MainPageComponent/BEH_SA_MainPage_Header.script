to ValidateIntervalBar VarDate,VarTime,count

	//set VarDate to "08/21/18"
	//set Vartime to "1:30"
	//set count to "3"  //Enter only odd numbers
	put Topright of ImageRectangle( "BEH_SA_MainPageComponent/Badge_Precautions/Badge_Precautions") into Topright--Setting(x1,y1)
	insert Topright +(500,35) into Bottomleft 
	put  ReadText(Topright,Bottomleft) into DateTimeRange
	//log DateTimeRange
	//put (Topright,Bottomleft) into DateTimeRange
	
	//set the searchrectangle to ()
	
	//put DateTimeRange into Data1
	replace "\t" by return in DateTimeRange
	replace space by return in DateTimeRange
	log DateTimeRange
	
	put line count of (DateTimeRange) into Date1
	put line count+3 of ( DateTimeRange) into time2
	//delete every occurrence of "(" in Date1
	log (Date1)
	log (time2)
	
	if  trim(VarDate) equals trim(Date1)
		log "success"
	else
		log "Failure"
	end if
	
	if  trim(VarTime) is greater than or equal to trim(time2)
		log "success"
	else
		log "Failure"
	end if
	
end ValidateIntervalBar

