to ValidateIntervalCel VarDate,VarTime

	set VarDate to "08/13/18"
	set Vartime to "06:59"
	
	
	put (726,291,858,340) into var
	
	
	repeat 4 times
		insert readtext(var) into value
		put var+(131, 0, 131,0) into var
	end repeat
	
	replace "\t" by retun in value
	log value
	
	put line 1 of value into time1
	put line 2 of value into time2
	delete every occurrence of "(" in time1
	log time1
	
	log time2
	
	if  trim(VarDate) contains trim(time1)
		log "success"
	else
		log "Failure"
	end if
	
	if  trim(VarTime) is greater than or equal to trim(time2)
		log "success"
	else
		log "Failure"
	end if
	
end ValidateIntervalCel	
